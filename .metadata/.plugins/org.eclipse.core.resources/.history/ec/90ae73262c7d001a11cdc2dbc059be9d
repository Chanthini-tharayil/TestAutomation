package prepstring1;

public class maxconsecrepeating {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a= new int[256];
		String s="aaaabbcbbb";
		int max=Integer.MIN_VALUE;
		for(int i=0;i<s.length();i++)
			a[s.charAt(i)]++;
		for(int i=0;i<a.length;i++)
		{
			if(max<a[i])
				max=a[i];
		}
		System.out.println(max);

	}

}
