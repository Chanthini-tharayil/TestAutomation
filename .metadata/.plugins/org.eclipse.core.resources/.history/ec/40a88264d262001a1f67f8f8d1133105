package Hackerrank;

public class cloudjumping {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] c= {0,0,0,1,1,0,0};
		
		int count=0;
		for(int i=0;i<c.length;i++)
		{
		    if(c[i]==0 && c[i+1]==0)
		    	{
		    		count++;
		    	}
		    
		    

		    }
		System.out.println(count);

	}

}
