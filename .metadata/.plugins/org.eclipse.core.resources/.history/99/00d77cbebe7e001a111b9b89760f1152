package prep1;

public class subarraywithzerosum {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int[] a= {4,2,-3,-1,0,4};
int start=0,end=0;
for(int i=0;i<a.length;i++)
{
	for(int j=i+1;j<a.length;j++)
	{
		if(a[i]+a[j]==0)
		{
			start=i;
			end=j;
		}
	}
}
for(int i=start;i<=end;i++)
	System.out.println(a[i]);
	}
	

}
