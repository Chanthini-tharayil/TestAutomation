package prep1;

public class noofsubsetswithproductlessthank {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]={2, 4, 5, 3};
		int k=12;
		for(int i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length;j++)
			{
				if(a[i]*a[j]<=12)
					System.out.println(a[i]+" "+a[j]);
			}
		}
		

	}

}
