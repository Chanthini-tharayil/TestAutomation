package Hackerrank;

public class Occurence {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s="abab";
		 long n=10;
		 int count=0;
		
		 StringBuilder s1= new StringBuilder();
	        for(int i=0;i<n;i++)
	        {
	            s1.append(s);
	        }
	        System.out.println(s1);
	        String test=s1.toString();
	       
	        for(int i=0;i<n;i++)
	        {
	            if(test.charAt(i)=='a')
	            count++;
	        }
		 
	        System.out.println(count);

	}

}
