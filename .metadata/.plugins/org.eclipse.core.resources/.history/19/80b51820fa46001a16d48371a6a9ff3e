package dataStructures;

public class Fibonacciseries {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	int value=fib(10);
	

	}
	public static int fib(int n)
	{
		System.out.println(n);
		if(n<=0)
			return 0;
		else if(n==1)
			return 1;
		else 
		return fib(n-1)+fib(n-1);
		
	}

}
