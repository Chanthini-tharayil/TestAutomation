package prep;

public class findsecondlargest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int[] a = {12, 35, 1, 10, 34, 1};
int first=0,second=0;
for(int i=0;i<a.length;i++)
{
	if(a[i]>first)
	{
		second=first;
		first=a[i];
	}
	
}
System.out.println(second);


	}

}
