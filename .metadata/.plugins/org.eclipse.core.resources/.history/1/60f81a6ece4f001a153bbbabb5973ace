package Careercup;

public class ReverseStringFromIntervals {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String test="chanthin";
		String test1="";
		int j=0,i=0;
		while(i<=test.length())
		{   
			j=i+2;						
			while(j>=i&&j<=test.length())
			{
				
				test1+=test.charAt(j);
				j--;
				
			}
			i=i+2;
			
		}
		System.out.println(test1);
	}

}
