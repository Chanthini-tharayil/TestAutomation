package prepstring1;

import java.util.Arrays;

public class pangram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Boolean[] b= new Boolean[26];
		Arrays.fill(b, false);
		int index=0;
		String test ="aabbcc";
		for(int i=0;i<test.length();i++)
		{
			index=test.charAt(i)-'a';
			b[index]=true;
		}
			

	}

}
