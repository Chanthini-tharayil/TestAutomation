package Hackerrank;

import java.util.HashMap;
import java.util.HashSet;

public class hackerrankstrin1 {
	public static void main(String[] args) {
	String test1="rhbaasdndfsdskgbfefdbrsdfhuyatrjtcrtyytktjjt";
	String test="hackerrank";
	StringBuilder s1= new StringBuilder();
	char[] a =test.toCharArray();
	char[] b=test1.toCharArray();
	HashSet<Character> s =new HashSet();
	for(int i=0;i<test.length();i++)
	{
		s.add(test.charAt(i));
	}
	int count=0;
	for(int i=0;i<test1.length();i++)
	{
		if(s.contains(test1.charAt(i)))
		s1.append(test1.charAt(i));
	}
	System.out.println(s1.equals(test));
	
	}
	
}
