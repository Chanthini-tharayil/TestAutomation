package prep;

public class moveallpositive {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a = { -2, -3, 0, -1, 0, 1, 5, -3 };
		int j=0,temp;
		for(int i=0;i<a.length;i++)
		{
			if(a[i]<0 || a[i]==0)
				{
				temp=a[j];
				a[j]=a[i];
				a[i]=temp;
				j++;
				}
		}
		for(int i=0;i<a.length;i++)
			System.out.println(a[i]);

	}

}
