package prep;

public class moveallpositive {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] a = { -2, -3, 4, -1, -2, 1, 5, -3 };
		int end=a.length-1,temp;
		for(int i=0;i<a.length;i++)
		{
			if(a[i]>0)
			{
				temp=a[i];
				a[i]=a[end];
				a[end]=temp;
				end--;
			}
		}
		for(int i=0;i<a.length;i++)
			System.out.println(a[i]);

	}

}
