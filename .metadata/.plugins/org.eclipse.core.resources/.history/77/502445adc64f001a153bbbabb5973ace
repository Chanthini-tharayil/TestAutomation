package Careercup;

public class ReverseStringFromIntervals {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String test="chanthini";
		String test1="";
		int j=0,i=0;
		while(i+2<=test.length())
		{
			j=i+3;						
			while(j>=i)
			{
				
				test1+=test.charAt(j);
				j--;
				
			}
			i=i+3;
			
		}
		System.out.println(test1);
	}

}
