package BasicJava;

public class Minmultidimensionarray {
	public static void main(String[] args) {
	
		int a[][]=new int[2][2];
		a[0][0]=1;
	    a[0][1]=2;
		a[1][0]=3;
		a[1][1]=4;
		int test=0;
		for(int i=0;i<2;i++)
		{
			for(int j=0;j<2;j++)
			{
				for(int k=j+1;k<2;k++)
				{
				if(a[i][j]<a[i][k])
				{
					test=a[i][j];
				}
			}
		}
		
	}
System.out.println(test);
}
}
