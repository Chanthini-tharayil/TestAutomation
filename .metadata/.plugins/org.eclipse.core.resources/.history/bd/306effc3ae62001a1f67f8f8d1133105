package Hackerrank;

public class socksmatching {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] t= {10,10,20,30,20,30,50,60};
		int pair=1,count=0;
		for(int i=0;i<t.length;i++)
		{
			for(int j=i+1;j<t.length;j++)
			{
				if(t[i]==t[j])
					pair++;
			}
			if(pair%2==0)
				count++;
		}
		System.out.println(count);

	}

}
